[Unit]
 Description = Java Service
 After network.target = an_service.service
[Service]
 Type = forking
 Restart=always
 RestartSec=1
 SuccessExitStatus=143
 ExecStart = /usr/local/bin/an_service.sh start
 ExecStop = /usr/local/bin/an_service.sh stop
 ExecReload = /usr/local/bin/an_service.sh reload
[Install]
 WantedBy=multi-user.target